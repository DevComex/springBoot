<script>
    function formatDate(nS)   {
        var date = new Date(nS);
        Y = date.getFullYear() + '年';
        M = (date.getMonth()+1 < 10 ? '0'+(date.getMonth()+1) : date.getMonth()+1) + '月';
        D = date.getDate() + '日';
        return Y+M+D;
    };
    //多行滚动
    function enveScroll(){
        var _wrap = $('ul.mulitline');//定义滚动区域
        var _interval = 2000;//定义滚动间隙时间
        var _moving;//需要清除的动画
        _moving = setInterval(function () {
            var _field = _wrap.find('li:first');//此变量不可放置于函数起始处,li:first取值是变化的
            var _h = _field.height()+20;//取得每次滚动高度
            _wrap.animate({ marginTop: -_h + 'px' }, 500, function () {//通过取负margin值,隐藏第一行
                _wrap.css('marginTop', 0).append(_field);//隐藏后,将该行的margin值置零,并插入到最后,实现无缝滚动
            })
        }, _interval)
    };
    function allPrizes(){
        $.ajax({
            url: "/wdrankinglist2017/prizes/show",
            type: "GET",
            dataType: "json",
            data:{
                r: Math.random()
            },
            success: function (d) {
                if(d.isSuccess&& d.rows.length>0){
                    var html="";
                    for(var i=0;i<d.rows.length;i++){
//                        var  dd=new  Date(d.rows[i].winningAt);formatDate(dd)
                        html+='<li>恭喜玩家"'+d.rows[i].roleName+'"在抽奖活动中获得"'+d.rows[i].prizeName+'"</li>';
                    };
                    $(".userPrize").html(html);
                };
            },
            error:function(){
                alert("异常错误，请重试！");
            },
            complete:function(){
                if($(".userPrize li").length>5){
                    var liH=($(".mulitline li").eq(0).height()+20)*5;
                    $(".js_scrollCon").css("height",liH+"px");
                    enveScroll();
                };
            }
        });
    };
    function winPrizeFn(point, message,cardCode, defaults) {
        var loHas="";
        var cardCode_kh,cardCode_pas;
        if(cardCode!=null)
        {
            cardCode_kh=cardCode.toString().split(":")[0];
            cardCode_pas=cardCode.toString().split(":")[1];
        };
        switch (point) {
            case 0:
            case 1:
            case 2:
            case 4:
                $(".bgTop span").html("恭喜您");
                $(".cp2 span").show();
                $(".tcT").css("top","2%");
                $(".Tp .cont .cp2 i").html(message);
                $("#maskId,.uploadWord,.cont").show();
                allPrizes()
                break;
            case 3:
                $(".bgTop span").html("继续努力");
                $(".cp2 span").hide();
                $(".tcT").css("top","2%");
                $(".Tp .cont .cp2 i").html("谢谢参与");
                $("#maskId,.uploadWord,.cont").show();
                break;
            case  5:
            case  6:
            case  7:
            case  8:
            case  9:
            case 10:
            case 11:
                $(".bgTop span").html("恭喜您");
                $(".cp2 span").show();
                $(".tcT").css("top","0");
                $(".Tp .cont2 .cp2 i").html(message);
                $("#maskId,.uploadWord,.cont2").show();
                allPrizes()
            default:
        };
        $(defaults.Btn).attr(defaults.BtnAttr, defaults.BtnStateYes);
        $(".myAwards").attr("isc","1");
    };

    $(function(){
        allPrizes();
        //获取抽奖次数
        $.ajax({
            url: "/wdrankinglist2017/lottery",
            type: "GET",
            dataType: "json",
            data: {
                r: Math.random(),
                openId: openIdFormat()
            },
            success:function(d){
                $(".num").html(d.data);
            },
            error:function(){
                alert("异常错误，请重试!")
            }
        });

        $("#js_start").click(function(){
            $(document).rotate({
                ajaxData:{
                    openId: openIdFormat()
                }
            });
        });

        $(".Tp .sure").click(function(){
            $("#maskId,.uploadWord,.tcAl").hide();
        });
        $(".sure2").click(function(){
            var xm=$(".cont2 .xm").val(), sj=$(".cont2 .sj").val(), dz=$(".cont2 .dz").val();
            if(xm==""||sj==""||dz==""){
                alert("信息不能为空！");
                return false;
            };
            if(!(/^1[34578]\d{9}$/.test(sj))){
                alert("手机号码格式不对，请重填");
                return false;
            };
            $.ajax({
                url: "/wdrankinglist2017/address",
                type: "POST",
                dataType: "json",
                data:{
                    r: Math.random(),
                    openId: openIdFormat(),
                    activityId: 466,
                    userName : xm,
                    userPhone :sj,
                    userAddress :dz
                },
                success: function (d) {
                    if(d.isSuccess){
                        alert("添加成功");
                    }else{
                        alert("添加失败");
                    }
                    $("#maskId,.uploadWord,.tcAl").hide();
                },
                error:function(){
                    alert("异常错误，请重试！");
                }
            });
        });
        $(".myAwards").click(function(){
           if($(".myAwards").attr("isc")=="0"){
               return false;
           }
            $(".bgTop span").html("中奖记录");
            var html="";
            var indexLi=1;
            $.ajax({
                url: "/wdrankinglist2017/prizes",
                type: "GET",
                dataType: "json",
                data:{
                    r: Math.random(),
                    openId: openIdFormat()
                },
                success: function (d) {
                    if(d.isSuccess&& d.rows.length>0){
                        for(var i=0;i<d.rows.length;i++){
                            if(d.rows[i].prizeName!="谢谢参与"){
                                var  dd=new Date(d.rows[i].winningAt);
                                html+="<p>"+(indexLi)+".&nbsp;&nbsp;"+ d.rows[i].prizeName+"&nbsp;&nbsp;"+formatDate(dd)+"</p>";
                                indexLi++
                            }

                        };
                        $(".zjjl").html(html);
                    };
                    $(".tcT").css("top","2%");
                    $("#maskId,.uploadWord,.cont4").show();
                },
                error:function(){
                    alert("异常错误，请重试！");
                }
            });
        });
    });
</script>
