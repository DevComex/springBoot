    <script type="text/javascript">
        $(function () {
			$.ajax
			({
			    url: 'getUserLoginAccount',
			    type: "get",
			    data: {r: Math.random()},
			    dataType: "json",
				async:false,
			    success: function (d) {
					if (d.isSuccess) {
                        $(".js_act").html(d.data);
                    }
			    }
			});
            $(".js_getifon").click(function () {
                $.ajax
				({
                    url: 'updateInfo',
                    type: "get",
                    data: {
                        serverName: $(".js_serlist").find("option:selected").text(),
                        serverId: $(".js_serlist").val(),
                        roleSex: $(".js_sex").val(),
                        netType: $(".js_nettype").val(),
                        r: Math.random()
                    },
                    dataType: "json",
                    success: function (d) {
                        getInfo()
                    }
                });
            });
            function getserlist(serverName) {
                $.ajax
				({
                    url: 'getXWBServer',
                    type: "get",
                    data: {
                        domainNum: serverName,
                        r: Math.random()
                    },
                    dataType: "json",
                    success: function (d) {
                        var serlist = "";
                        if (d.length > 0) {
                            for (var i = 0; i < d.length; i++) {
                                serlist += '<option value="' + d[i].Code + '">' + d[i].ServerName + '</option>';
                            }
                        }
                        $(".js_serlist").html(serlist);
                        getInfo();
                    }
                });
            }
            getserlist("1");
            $(".js_nettype").change(function () {
                getserlist($(this).val());
            });
            function getInfo() {
                $.ajax
				({
                    url: 'getInfo ',
                    type: "get",
                    data: {
                        serverName: $(".js_serlist").find("option:selected").text(),
                        serverId: $(".js_serlist").val(),
                        roleSex: $(".js_sex").val(),
                        netType: $(".js_nettype").val(),
                        r: Math.random()
                    },
                    dataType: "json",
                    success: function (d) {
                        if (d.isSuccess) {
                            $(".js_bindser").html(d.data.serverName);
                            var roleSex = "";
                            if (d.data.roleSex == "男") {
                                roleSex="帅哥"
                            } else {
                                roleSex="美女"
                            }
                            $(".js_bindsex").html(roleSex)
                        }
                    }
                });
            };
             checkLogin({
                success: function (d) {
                    $(".js_isloginshow").show();
                    $(".js_isloginhide").hide();
                },
                failed: function () {
                    $(".js_isloginshow").hide();
                    $(".js_isloginhide").show();
                }
            });
        });
    </script>
