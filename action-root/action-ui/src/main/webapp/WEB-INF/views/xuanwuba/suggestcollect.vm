<!-- 视频征集提交JS验证 -->
<script  type="text/javascript">
$(function(){
	$("#name").focus(function(){
		if($("#name").val().trim() == "请输入名称"){
			$("#name").val("");
		}
	})
	
	$("#profile").focus(function(){
		if($("#profile").val().trim() == "请输入内容"){
			$("#profile").val("");
		}
		
	})


    $(".zjSubmit").click(function(){
		
		//验证是否为空，为空获得焦点
		if($("#name").val().trim() == "" || $("#name").val().trim() == "请输入名称"){
			alert("建议名称不能为空！");
			$("#name").focus();
		}else if($("#profile").val().trim() == "" || $("#profile").val().trim() == "请输入内容"){
			alert("建议简介不能为空！");
			$("#profile").focus();
		}else{
			$.ajax
			({
				url:'collectUpload',
				type:'POST',
				data:$("#suggestInfoForm").serialize(),
				dataType:'json',
				success:function(d){
					alert(d.message);
					if(d.isSuccess){
						window.location.href="collectIndex";
					}
				},
				error:function(){
					alert("网络连接超时！")
				}
			})
		}
	})
})
</script>
