<body>
    <section class="vbox">
        <!--公用头部  begin-->
         <header class="bg-dark dk header navbar navbar-fixed-top-xs js_template_top"></header>
        <section>
            <section class="hbox stretch">
                <aside class="bg-dark lter aside-md hidden-print js_template_left" id="nav"></aside>
                <section id="Section1">
                    <section class="vbox js_template_right">
                        <!--顶部标题导航 begin-->
                        <header class="header bg-white b-b clearfix">
                            <div class="">

                                <div class="col-sm-2 m-b-xs">
                                    <h4 class="m-t bold"></h4>
                                </div>
                                <div class="col-sm-2 m-b-xs m-t-sm">
                                    <div class="input-group">
                                        
                                    </div>
                                </div>
                            </div>
                        </header>
                        <!--顶部标题导航 end-->
						
        <!--公用头部  end-->
                <section id="content">
                    <section class="vbox">
                        <!--头部功能-->
                        <!--<header class="header bg-white b-b clearfix">
                            <div class="row m-t-sm">    
                                <div class="col-sm-2 m-b-xs">
                                    <div class="input-group">
                                        <input type="text" class="input-sm form-control" placeholder="账号">
                                    </div>
                                </div>
                                <div class="col-sm-2 m-b-xs">
                                    <div class="input-group">
                                        <input type="text" class="input-sm form-control js_startTimeCtr" readonly placeholder="签到时间">
                                    </div>
                                </div>     
                                <div class="col-sm-1 m-b-xs">
                                    <span class="input-group-btn">
                                        <button class="btn btn-sm btn-info" type="button">查询</button>
                                    </span>
                                </div>
    
                            </div>
                        </header>-->
                        <!--表格主体-->
                        <section class="scrollable padder" style="padding-top:20px;">
                            <div class="m-b-md">
                            </div>
                            <!--数据表格  begin -->
                            <section class="panel panel-default" >
                                <div class="table-responsive">  
                                    <table class="table table-striped m-b-none">
                                        <thead>
                                            <tr>
                                                <th>物品编号</th>
                                                <th>物品名称</th>
                                                <th>物品图片</th>
                                                <th>物品说明</th>
												<th>对应价钱</th>
                                                <th>物品兑换所需积分</th>
                                            </tr>
                                        </thead>
                                        <tbody>
											#foreach($!good in $!goodsList)
                                            <tr>
                                                <td>$!good.goodsCode</td>
                                                <td>$!good.goodsName</td>
                                                <td><a target="_blank" href="$!good.goodsPic">$!good.goodsPic</a></td>
                                                <td>$!good.goodsState</td>
												<td>$!good.price</td>
                                                <td>$!good.creditsCost</td>
                                            </tr>
											#end
                                        </tbody>
                                    </table>

                                </div>
                            </section>
                            <!--数据表格  end -->
                        </section>
                        <!--翻页功能-->
                        <footer class="footer bg-white b-t">
                            <div class="row dataTables_wrapper">
                                <div class="col-sm-6">
                                    <div class="dataTables_info" id="DataTables_Table_0_info">共$!{page.totalRecords}条 第$!{page.pageNo}/$!{page.totalPages}页</div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="dataTables_paginate paging_full_numbers" id="DataTables_Table_0_paginate">
										#if(${page.pageNo} == $!{page.topPage})
                                        <a href="goods?pageNum=$!{page.topPage}" class="first paginate_button paginate_button_disabled" tabindex="0" id="DataTables_Table_0_first">首页</a>
										<a href="goods?pageNum=$!{page.previousPageNo}" class="previous paginate_button paginate_button_disabled" tabindex="0" id="DataTables_Table_0_previous">上一页</a>
										#else
										<a href="goods?pageNum=$!{page.topPage}" class="first paginate_button" tabindex="0" id="DataTables_Table_0_first">首页</a>
										<a href="goods?pageNum=$!{page.previousPageNo}" class="previous paginate_button" tabindex="0" id="DataTables_Table_0_previous">上一页</a>
										#end
										<span>
											#foreach($param in $!page.pageNumArray)
												#if($param == $page.pageNo)
													<a href="goods?pageNum=$param" class="paginate_active" style="background-color:#D1D1D1" tabindex="0">$param</a>
												#else
													<a href="goods?pageNum=$param" class="paginate_button" tabindex="0">$param</a>
												#end
											#end
											
										</span>
										#if(${page.pageNo} == $!{page.bottomPageNo})
										<a href="goods?pageNum=$!{page.nextPageNo}" class="next paginate_button paginate_button_disabled" tabindex="0" id="DataTables_Table_0_next">下一页</a>
										<a href="goods?pageNum=$!{page.bottomPageNo}" class="last paginate_button paginate_button_disabled" tabindex="0" id="DataTables_Table_0_last">末页</a>
										#else
										<a href="goods?pageNum=$!{page.nextPageNo}" class="next paginate_button" tabindex="0" id="DataTables_Table_0_next">下一页</a>
										<a href="goods?pageNum=$!{page.bottomPageNo}" class="last paginate_button" tabindex="0" id="DataTables_Table_0_last">末页</a>	
										#end
                                    </div>
                                </div>
                            </div>
                        </footer>
                    </section>
                </section>
                <a href="#" class="hide nav-off-screen-block" data-toggle="class:nav-off-screen" data-target="#nav"></a>
                <aside class="bg-light lter b-l aside-md hide" id="notes">
                    <div class="wrapper">Notification</div>
                </aside>
            </section>
        </section>
    </section>
             </section>
         </section>
     <script src="${rc.contextPath}/xwbresources/Script/app.v2.js"></script>
    <script type="text/javascript">
        $(function () {
            //时间控件--开始
            $(".js_startTimeCtr").datepicker({
                format: 'yyyy-mm-dd',
                startView: 2,   //默认打开是年试图
                autoclose: true,  //选择后自动关闭
                todayBtn: true,  //选择今天的按钮
                minView: 2   //只能选到年试图  （0 代表能选到分钟、1 代表能选到小时、2 代表能选到日 ）
            })
            //时间控件--结束
        });
    </script>
