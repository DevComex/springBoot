"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.gyyx.oa.internal.dao.newyear.IChipLogMapper">
	
	<insert id="insertLog" parameterType="UserChipBean">
		INSERT INTO  user_chip_log (user_chip_name,user_chip_type,user_chip_num,user_chip_time)  
