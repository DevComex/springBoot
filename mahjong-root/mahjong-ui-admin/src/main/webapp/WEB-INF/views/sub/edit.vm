<!doctype html>
<html>
  <head>
    <meta content="IE=11.0000" http-equiv="X-UA-Compatible">
      <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
        <meta name="viewport" content="width = device-width ,height = device-height ,initial-scale = 1,minimum-scale = 1,maximum-scale = 1,user-scalable =no" />
        <title>小鸡麻将</title>
        <link rel="stylesheet" type="text/css" href="../resources/css/common.css" />
        <link rel="stylesheet" type="text/css" href="../resources/css/main.css" />
        <script>var rc = "${rc.contextPath}";</script>
        <script src="../resources/js/common/jquery-1.12.4.min.js" type="text/javascript"></script>
        <script type="text/javascript">
          $(function () { $(".reg_form_web").AddHead(); });
        </script>
      </head>
  <body>
    <div class="topbar-wrap white">
      <div class="topbar-inner clearfix">
        <div class="topbar-logo-wrap clearfix">
          <ul class="navbar-list clearfix">
            <li>添加下级</li>
          </ul>
        </div>
      </div>
    </div>
    <div class="container clearfix">
      <div class="main-wrap noleft">
        <div class="result-wrap">
          <form method="post" action="" class="reg_form_web" name="form1">
            <p class="item_p">
              <label>所属游戏：</label>
              <span class="ipt_box">
                            <select class="ipt" name="game" data-tip="湖北麻将">
                                <option value="1" selected="selected">湖北麻将</option>
                            </select>
              </span>
            </p>
            <p class="item_p">
                        <label>代理等级：</label>
                        <span class="ipt_box">
                            <select class="ipt js_agents" name="agent" data-tip="代理等级">
                                <option value="0">全部</option>
                                <option value="1">推广员</option>
                                <option value="2">客服员</option>
                                <option value="3">局头</option>
                            </select>
                        </span>
                        <span class="error tip"></span>
             </p>
            <p class="item_p">
                        <label>账号：</label>
                        <span class="ipt_box">
                            <input class="ipt" type="text" name="account" data-tip="账号" placeholder="账号" value="" maxlength="16">
                        </span>
                        <span class="error tip"></span>
            </p>
            <p class="item_p">
              <label>密码：</label>
              <span class="ipt_box">
                <input class="ipt" type="password" name="password" data-tip="密码" placeholder="6-16位密码" value=""  maxlength="16">
                        </span>
              <span class="error tip"></span>
            </p>

            
            <p class="item_p">
              <label>姓名：</label>
              <span class="ipt_box">
                <input class="ipt" type="text" name="name" data-tip="姓名" placeholder="姓名" value="" maxlength="20"/>
              </span>
              <span class="error tip"></span>
            </p>
            <p class="item_p">
              <label>手机：</label>
              <span class="ipt_box">
                <input class="ipt" type="text" name="phone" data-tip="手机" placeholder="手机" value="" maxlength="11"/>
              </span>
              <span class="error tip"></span>
            </p>
            
            <p class="item_p">
              <label>地址：</label>
              <span class="ipt_box">
                <select class="ipt sel01 js_state capthaipt" name="province" data-tip="省份" id="province">
                  <option value="0" selected="">选择省份</option>
                </select>
              </span>
              <span class="error tip"></span>
            </p>
            <p class="item_p">
              <label>  </label>
              <span class="ipt_box">
                <select class="ipt js_state capthaipt mrl10" name="city" data-tip="市区" id="city">
                  <option value="0" selected="">选择城市</option>
                </select>
              </span>
              <span class="error tip"></span>
            </p>
           
            <p class="formbtn">
              <a href="javascript:;" class="btn btn-success js_submit">确认</a>
              <a href="javascript:;" class="btn btn-info js_back">取消</a>
            </p>
          </form>
        </div>
      </div>
    </div>
    <div class="botmenu white js_bottommenu"></div>
    <script src="../resources/js/menu/js/menuprojson.js"></script>
    <script src="../resources/js/common/city.js" type="text/javascript"></script>
    <script src="../resources/js/static-oa/addHead.js" type="text/javascript"></script>
  </body>
</html>
