<!doctype html>
<html>
<head>
    <meta content="IE=11.0000" http-equiv="X-UA-Compatible">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta name="viewport" content="width = device-width ,height = device-height ,initial-scale = 1,minimum-scale = 1,maximum-scale = 1,user-scalable =no" />
    <title>小鸡麻将</title>
    <link rel="stylesheet" type="text/css" href="../resources/css/common.css" />
    <link rel="stylesheet" type="text/css" href="../resources/css/main.css" />
    <script>var rc = "${rc.contextPath}";</script>
    <script src="../resources/js/common/jquery-1.12.4.min.js" type="text/javascript"></script>
    <script src="../resources/js/static-oa/purchaseHistory.js" type="text/javascript"></script>
    <script type="text/javascript">
      $(function () { $(".reg_form_web").PurchaseHistory(); });
    </script>
</head>
<body>
    <div class="topbar-wrap white">
        <div class="topbar-inner clearfix">
            <div class="topbar-logo-wrap clearfix">
                <ul class="navbar-list clearfix">
                    <li>个人详细进货记录</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container clearfix">
        <div class="main-wrap noleft">
            <div class="result-wrap">
                <form method="post" action="" class="reg_form_web">
                    <p>
                        <label>月份：</label>
                        <span class="ipt_box">
                            <select class="ipt js_month" name="month" data-tip="月份">
                                <option value="1">1月</option>
                                <option value="2">2月</option>
                                <option value="3">3月</option>
                                <option value="4">4月</option>
                                <option value="5">5月</option>
                                <option value="6">6月</option>
                                <option value="7">7月</option>
                                <option value="8">8月</option>
                                <option value="9">9月</option>
                                <option value="10">10月</option>
                                <option value="11">11月</option>
                                <option value="12">12月</option>
                            </select>
                        </span>
                        <span class="error tip"></span>
                    </p>
                    <p class="formbtn">
                        <a href="javascript:;" class="btn btn-success js_search">查询</a>
                    </p>
                    <p class="total js_sum"></p>
                    <table class="result-tab">
                        <thead>
                            <tr>
                              <th>购买数量</th>
                              <th>赠送数量</th>
                              <th>进货时间</th>
                            </tr>
                        </thead>
                        <tbody class="js_pagetab">
                        <tr><td colspan="3">暂无数据</td></tr>
                        </tbody>
                    </table>
                  <p class="formbtn dis js_next">
                    <a href="javascript:;" class="btn btn-success js_page">下一页</a>
                  </p>
                </form>
            </div>
        </div>
    </div>
    <div class="botmenu white js_bottommenu"></div>
    <script src="../resources/js/menu/js/menuprojson.js"></script>
</body>
</html>
