<!doctype html>
<html>
<head>
    <meta content="IE=11.0000" http-equiv="X-UA-Compatible">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta name="viewport" content="width = device-width ,height = device-height ,initial-scale = 1,minimum-scale = 1,maximum-scale = 1,user-scalable =no" />
    <title>小鸡麻将</title>
    <link rel="stylesheet" type="text/css" href="../resources/css/common.css" />
    <link rel="stylesheet" type="text/css" href="../resources/css/main.css" />
    <script>var rc = "${rc.contextPath}";</script>
    <script src="../resources/js/common/jquery-1.12.4.min.js" type="text/javascript"></script>
    <script src="../resources/js/static-oa/salesCards.js" type="text/javascript"></script>
    <script type="text/javascript">
        $(function () { $(".reg_form_web").RechargeCard(); });
    </script>
</head>
<body>
    <div class="topbar-wrap white">
        <div class="topbar-inner clearfix">
            <div class="topbar-logo-wrap clearfix">
                <ul class="navbar-list clearfix">
                    <li>销售房卡</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container clearfix">
        <div class="main-wrap noleft">
            <div class="result-wrap">
                <form method="post" action="" class="reg_form_web">
                    <div class="js_fir">
                        <p>
                            <label>个人库存：</label>
                            <span class="txtform js_inventory" data-blc=""></span>
                        </p>
                        <p class="item_p">
                            <label>账号：</label>
                            <span class="ipt_box">
                                <input class="ipt short" name="account" data-tip="下级账号" type="text" placeholder="请输入下级账号" value=""  maxlength="11">
                            </span>
                            <span class="error tip"></span>
                            <a href="javascript:;" class="btn btn-info btnlater js_checkid">查找</a>
                        </p>
                        
                    </div>
                    <div class="js_sed dis">
                        <p>
                            <label>账号：</label>
                            <span class="txtform js_userid"></span>
                        </p>
                        <p>
                            <label>姓名：</label>
                            <span class="txtform js_nickName"></span>
                        </p>
                        <p>
                            <label>手机号：</label>
                            <span class="txtform js_phone"></span>
                        </p>
                        <p>
                            <label>等级：</label>
                            <span class="txtform js_role"></span>
                        </p>
                        <p>
                            <label>库存：</label>
                            <span class="txtform js_balance"></span>
                        </p>
                        <p class="item_p">
                            <label>房卡数量：</label>
                            <span class="ipt_box">
                                <input class="ipt short js_cardNum" type="text" data-tip="充卡数量" name="amount" placeholder="请输入充卡数量" value=""  maxlength="7"/>
                            </span>
                            <span class="error tip"></span>
                           
                        </p>
                        <p>
                            <label>是否赠送：</label>
                            <select class="ipt capthaipt js_select">
                                 <option value="1">是</option>
                                 <option value="0">否</option>
                            </select>

                        </p>
                        <p class="item_p js_gifts">
                            <label>赠送数量：</label>
                            <span class="ipt_box">
                                <input class="ipt short js_gift" type="text" data-tip="赠送数量" name="gift" placeholder="赠送数量" value=""  maxlength="7"/>
                            </span>
                            <span class="error tip"></span>
                        </p>
                        <p class="formbtn">
                            <a href="javascript:;" class="btn btn-success js_recharge">充值</a>
                            <a href="javascript:;" class="btn btn-info js_prev">取消</a>
                        </p>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <div class="botmenu white js_bottommenu"></div>
    <script src="../resources/js/menu/js/menuprojson.js"></script>
</body>
</html>
